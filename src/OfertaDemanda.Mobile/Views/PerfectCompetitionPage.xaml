<?xml version="1.0" encoding="UTF-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:views="clr-namespace:OfertaDemanda.Mobile.Views"
            x:Class="OfertaDemanda.Mobile.Views.PerfectCompetitionPage">
    <views:MarketPage BindingContext="{Binding Market}"
                      Title="{Binding Localization[Tab_Market]}" />
    <views:FirmPage Title="{Binding Localization[Tab_Firm]}" />
    <views:ElasticityPage BindingContext="{Binding Elasticity}"
                          Title="{Binding Localization[Tab_Elasticity]}" />
</TabbedPage>
