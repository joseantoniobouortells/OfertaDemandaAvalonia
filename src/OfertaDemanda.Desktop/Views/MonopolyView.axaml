<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="using:OfertaDemanda.Desktop.Views"
             x:CompileBindings="False"
             mc:Ignorable="d"
             x:Class="OfertaDemanda.Desktop.Views.MonopolyView">
  <TabControl TabStripPlacement="Top">
    <TabItem Header="{Binding Localization[Tab_Model]}">
      <views:MonopolyModelView DataContext="{Binding Monopoly}" />
    </TabItem>
    <TabItem Header="{Binding Localization[Tab_Welfare]}">
      <views:MonopolyWelfareView DataContext="{Binding Monopoly}" />
    </TabItem>
  </TabControl>
</UserControl>
